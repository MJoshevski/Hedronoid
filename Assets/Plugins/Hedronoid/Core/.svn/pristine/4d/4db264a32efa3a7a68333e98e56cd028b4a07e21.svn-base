using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public static class NNMonobehaviourExtensions
{

    public static Coroutine StopCoroutineSafely(this MonoBehaviour mb, Coroutine c)
    {
        if (c != null)
        {
            mb.StopCoroutine(c);
        }
        return null;
    }
}
